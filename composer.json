{
    "name": "jumbojett/openid-connect-php",
    "description": "Bare-bones OpenID Connect client",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.1.3",
        "phpseclib/phpseclib" : "~2.0",
        "ext-json": "*",
        "ext-curl": "*",
        "illuminate/container": "~5.8.0",
        "illuminate/session": "~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "roave/security-advisories": "dev-master"
    },
    "archive" : {
        "exclude" : [
            ".*"
        ]
    },
    "autoload" : {
         "classmap": [ "src/"]
    },
  "config" : {
    "platform": {
      "php": "7.1.33"
    }
  }
}
